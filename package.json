{
  "name": "taskpulse",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "npm run build && (npx vite-bundle-analyzer dist 2>/dev/null || echo 'Bundle analyzer not available, install vite-bundle-analyzer to use this feature')",
    "performance:test": "node scripts/test-performance.js",
    "performance:dev": "npm run dev & sleep 5 && npm run performance:test",
    "test:performance": "node scripts/test-performance.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run && npm run test:e2e",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/tests/unit",
    "test:integration": "vitest run --config=vitest.integration.config.ts",
    "test:e2e": "playwright test --config=src/tests/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=src/tests/config/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=src/tests/config/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=src/tests/config/playwright.config.ts --debug",
    "test:accessibility": "vitest run src/tests/accessibility",
    "test:security": "vitest run src/tests/security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:accessibility && npm run test:security"
  },
